<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DataTypes"
namespace="DataTypes.Mappings">
  <subclass name="ModelGroup">
    <property name="ModelGroupName" />
    <one-to-one name="Parent" foreign-key="ItemId" class="Datatypes.StoreTypes.ModelGroup"/>
    <bag name="Products" >
      <key column="modelID" />
      <one-to-many class="Datatypes.StoreTypes.ShippedProduct"/>
    </bag>
  </subclass>
</hibernate-mapping>